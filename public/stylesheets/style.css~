body {
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
}

/* Make Element Unselectable */
/* Add unselectable="on" Attribute For IE */
.unselectable {
	cursor: pointer;
	user-select: none;
	-o-user-select: none;
	-moz-user-select: none;
	-khtml-user-select: none;
	-webkit-user-select: none;
}

